<div class="cd-footer__section cd-footer__section--social cd-footer-social">
  {% block social_media %}

    <ul class="cd-footer-social__list">
      <li class="cd-footer-social__item">
        <a href="https://www.facebook.com/UNOCHA" class="cd-footer-social__link">
          <span class="visually-hidden">Facebook</span>

          <svg class="cd-icon cd-icon--facebook" aria-hidden="true" focusable="false" width="26" height="26">
            <use xlink:href="#cd-icon--sm-fb-def"></use>
          </svg>
          <svg class="cd-icon cd-icon--facebook hover-style" aria-hidden="true" focusable="false" width="26" height="26">
            <use xlink:href="#cd-icon--sm-fb-full"></use>
          </svg>
        </a>
      </li>
      <li class="cd-footer-social__item">
        <a href="https://twitter.com/unocha" class="cd-footer-social__link">
