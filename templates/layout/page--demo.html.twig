  h2.cd-styleguide,
  h3.cd-styleguide {
    display: block;
    padding: 1rem;
    background-color: var(--cd-blue-grey);
  }

  h2.cd-styleguide {
    font-size: 2.5rem;
    margin: 4rem 0;
  }

  h3.cd-styleguide {
    margin: 4rem 0;
    font-size: 2rem;
    width: 100%;
    border: 1px solid var(--cd-blue--bright);
    display: flex;
    justify-content: space-between;
    align-content: center;
  }

  h3.cd-styleguide > a[id*="cd-"] {
    flex: 1;
  }

  .cd-footer {
  --cd-footer-row-number: 6;
